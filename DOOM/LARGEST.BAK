Program largest;
(*El CHoDA
Baker 6th, *)

uses
    Crt;
type
fred=array [1..10] of integer
var
choda:fred;
x,y:integer;
begin (*main*)
clrscr;
for x=1 to 10 do
begin
writeln ('enter number ',x);
readln (choda[x]);
end;
for x=1 to 10 do
if choda[x]>y then
y:choda[x];
writeln (y,' is the largest number');
readln;
End. (*main*)

